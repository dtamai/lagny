@namespace("Kerala")
protocol Kerala {
    import idl "Header_v1.avdl";

    record AddSpending {
        Header header = {};
        string date = "2001-01-01";
        string currency = "unknown";
        int cents = 0;
        string pay_method = "unknown";
        string seller = "unknown";
        string category = "unknown";
        array<string> tags = [];
        string description = "unknown";
    }
}
